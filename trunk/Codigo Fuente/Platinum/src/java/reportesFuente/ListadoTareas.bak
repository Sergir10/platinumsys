	<queryString><![CDATA[select 
pd.fecha as fecha,
pd.cantidad as cantidad,
e.nombre_empleado||' '||e.apellido_empleado as Empleado,
ta.cod_tarea as CodTarea,
t.nombre_tarea as Tarea,
pd.tiempo_invertido as Tiempo,
od.cod_orden_trabajo_det as CodDetOt,
ot.cod_orden_trabjo as NroOt,
pp.descripcion as prodSubOt,
p.descripcion as prodOt

from produccion_diaria pd 
     join tarea_asignada ta on pd.cod_tarea_asignada = ta.cod_tarea_asignada
     join tarea t on ta.cod_tarea = t.cod_tarea
     join orden_trabajo_detalle od on ta.cod_det_orden_trabaj = od.cod_orden_trabajo_det
     join orden_trabajo ot on od.cod_orden_trabajo = ot.cod_orden_trabjo
     join producto pp on od.cod_producto = pp.cod_producto
     join producto p on ot.cod_producto_ot = p.cod_producto
     join empleado e on pd.cod_empleado = e.cod_empleado

where 1=1 $P!{parametros}
order by pd.fecha, ot.cod_orden_trabjo, od.cod_orden_trabajo_det, ta.cod_tarea_asignada
]]></queryString>

