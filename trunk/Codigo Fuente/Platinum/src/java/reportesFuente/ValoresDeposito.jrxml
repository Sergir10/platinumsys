	<queryString><![CDATA[select    e.cod_producto, 
          d.nombre as Deposito,
          p.descripcion as Producto,
          m.nombre as Marca,
          (p.costo_actual * e.cantidad_existencia) as ValorExistencia	        
from existencia e 
        join  producto p on e.cod_producto = p.cod_producto
        join  deposito d on e.cod_deposito = d.cod_deposito
        join tipo_producto t on p.cod_tipo_producto = t.cod_tipo_producto
        join marca m on p.cod_marca = m.cod_marca
where t.descripcion not like '%Gen%' $P!{parametros}
order by d.nombre, t.descripcion, p.descripcion]]></queryString>

