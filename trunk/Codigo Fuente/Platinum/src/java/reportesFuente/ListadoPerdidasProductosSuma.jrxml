	<queryString><![CDATA[select p.cod_producto as CodProducto, 
p.descripcion as Descripcion,
sum(per.cantidad_perdida) as Cantidad,
sum((per.cantidad_perdida*p.costo_actual)) as Total
from perdida per
join producto p on per.cod_producto = p.cod_producto
join marca m on p.cod_marca = m.cod_marca
join tipo_producto t on t.cod_tipo_producto = p.cod_tipo_producto
join empleado e on e.cod_empleado = per.cod_empleado
where 1=1 $P!{parametros}
group by p.cod_producto,p.descripcion
order by p.descripcion, p.cod_producto ]]></queryString>

