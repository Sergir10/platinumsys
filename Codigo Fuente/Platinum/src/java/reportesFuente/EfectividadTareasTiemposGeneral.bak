	<queryString><![CDATA[select nvl((select sum(te.tiempo) 
               from v_tareas_empleado te),0) as TiempoUtil, 
       
       nvl((select sum(te.tiempo) 
               from v_tareas_fallidas_empleado te),0) as TiempoFalla,
               
       nvl((100 * (select sum(te.tiempo) 
               from v_tareas_empleado te)/
		(nvl((select sum(te.tiempo) from v_tareas_empleado te),0)+
       		nvl((select sum(te.tiempo) from v_tareas_fallidas_empleado te),0))),0) as Efectividad,
           
           nvl((select sum(te.tiempo) from v_tareas_empleado te),0)
           +
       		 nvl((select sum(te.tiempo) from v_tareas_fallidas_empleado te),0)
           as suma       
           from dual]]></queryString>

