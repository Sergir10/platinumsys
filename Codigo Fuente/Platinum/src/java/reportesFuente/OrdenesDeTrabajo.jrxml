	<queryString><![CDATA[select 
ot.cod_orden_trabjo as NroOT, 
ot.descripcion as DescripcionOt, 
ot.Fecha_Inicial_Ot, 
case
	when ot.Estado_Ot = 'A' then 'Abierto'
	when ot.Estado_Ot = 'P' then 'En Proceso'
	when ot.Estado_Ot = 'T' then 'Terminado'
	else 'Cerrado'
end as Estado_OT,
e.nombre_empleado||' '||e.apellido_empleado as EmpleadoOt,
pOt.descripcion as ProductoOt, 
ot.cantidad_ot as CantOt, 
ot.cantidad_producida_ot as CantProdOt,
ot.cantidad_producida_ot * 100 / ot.cantidad_ot as Porcentaje,
oD.Cod_Orden_Trabajo_Det as CodDet,
case
	when oD.Estado = 'A' then 'Abierto'
	when oD.Estado = 'P' then 'En Proceso'
	when oD.Estado = 'T' then 'Terminado'
	else 'Cerrado'
end as EstadoDet,
oD.Cantidad as CantDet,
oD.Cantidad_Real CantProdDet,
oD.cantidad_Real * 100 / oD.cantidad as PorcentajeD,
od.fecha_alta,
pOd.descripcion

from orden_trabajo_detalle oD join producto pOd on od.cod_producto = pOd.cod_producto
	join orden_trabajo ot on od.cod_orden_trabajo = ot.cod_orden_trabjo
	join producto pOt on ot.cod_producto_ot = pOt.cod_producto
	join empleado e on ot.cod_empleado1 = e.cod_empleado
where 1=1 $P!{parametros}
order by ot.cod_orden_trabjo, ot.cod_orden_trabjo, od.estado]]></queryString>

