	<queryString><![CDATA[select per.fecha_perdida as Fecha,
e.nombre_empleado||' '||e.apellido_empleado as Empleado, 
p.cod_producto as CodProducto, 
p.descripcion as Descripcion, 
t.descripcion as Tipo,
 m.nombre as Marca, 
 per.cantidad_perdida as Cantidad, 
 p.costo_actual as CostoUnit, 
 (per.cantidad_perdida*p.costo_actual) as Total
from perdida per
join producto p on per.cod_producto = p.cod_producto
join marca m on p.cod_marca = m.cod_marca
join tipo_producto t on t.cod_tipo_producto = p.cod_tipo_producto
join empleado e on e.cod_empleado = per.cod_empleado
where 1=1 $P!{parametros}
order by t.descripcion, per.fecha_perdida
]]></queryString>

