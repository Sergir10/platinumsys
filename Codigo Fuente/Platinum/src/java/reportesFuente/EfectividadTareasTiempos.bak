	<queryString><![CDATA[select t.cod_tarea as CodTarea,t.nombre_tarea as Descripcion, 
       nvl((select sum(te.tiempo) 
               from v_tareas_empleado te 
               where te.cod_tarea = t.cod_tarea $P!{parametros}),0) as TiempoUtil , 
       
       nvl((select sum(te.tiempo) 
               from v_tareas_fallidas_empleado te 
               where te.cod_tarea = t.cod_tarea $P!{parametros}),0) as TiempoFalla,
               
       nvl((100 * (select sum(te.tiempo) 
               from v_tareas_empleado te 
               		where te.cod_tarea = t.cod_tarea $P!{parametros})/
		(nvl((select sum(te.tiempo) from v_tareas_empleado te 
               		where te.cod_tarea = t.cod_tarea $P!{parametros}),0)+
       		nvl((select sum(te.tiempo) from v_tareas_fallidas_empleado te 
               		where te.cod_tarea = t.cod_tarea $P!{parametros}),0))),0) as Efectividad,
           
           nvl((select sum(te.tiempo) from v_tareas_empleado te where te.cod_tarea = t.cod_tarea $P!{parametros}),0)
           +
       		 nvl((select sum(te.tiempo) from v_tareas_fallidas_empleado te where te.cod_tarea = t.cod_tarea $P!{parametros}),0)
           as suma       
                  
                  
        from tarea t
        order by nvl((100 * (select sum(te.tiempo) 
               from v_tareas_empleado te 
               		where te.cod_tarea = t.cod_tarea $P!{parametros})/
		(nvl((select sum(te.tiempo) from v_tareas_empleado te 
               		where te.cod_tarea = t.cod_tarea $P!{parametros}),0)+
       		nvl((select sum(te.tiempo) from v_tareas_fallidas_empleado te 
               		where te.cod_tarea = t.cod_tarea $P!{parametros}),0))),0) DESC]]></queryString>

