	<queryString><![CDATA[select e.cod_empleado as cod_vendedor, 
       e.apellido_empleado||', '||e.nombre_empleado as vendedor, 
       t.desc_tipo_abreviado as tipo,
       c.establecimiento||'-'||c.boca_expendio||'-'||lpad(to_char(c.numero_factura), 10, '0') as nro,
       c.fecha_factura as fecha,
       c.total_factura as total_factura
  from factura_cabecera c, empleado e, tipo_comprobante t
 where c.estado_factura != 'ANULADO'
   and e.cod_empleado = c.cod_empleado
   and t.cod_tipo = c.tipo_factura
   order by c.fecha_factura]]></queryString>

