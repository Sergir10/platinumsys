	<queryString><![CDATA[select p.descripcion as Producto,  sum(pro.cantidad) as Cantidad
from
produccion_diaria pro
join tarea_asignada ta on pro.cod_tarea_asignada = ta.cod_tarea_asignada
join orden_trabajo_detalle od on ta.cod_det_orden_trabaj = od.cod_orden_trabajo_det
join orden_trabajo ot on od.cod_orden_trabajo = ot.cod_orden_trabjo
join producto p on od.cod_producto = p.cod_producto
where p.cod_tipo_producto != 6 and ta.tarea_fin is not null $P!{parametros}
group by p.descripcion]]></queryString>

