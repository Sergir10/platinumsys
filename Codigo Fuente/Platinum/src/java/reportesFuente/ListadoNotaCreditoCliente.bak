	<queryString><![CDATA[select e.cod_cliente as cod_cliente,
e.apellido_cliente||', '||e.nombre_cliente as cliente,
t.desc_tipo_abreviado as tipo,
c.establecimiento||'-'||c.boca_expendio||'-'||lpad(to_char(c.num_nota_credto_cliente), 10, '0') as nro,
c.fecha_nota_credito as fecha,
c.total_nota_credito as total_nota
from nota_credito_cli_cabecera c, cliente e, tipo_comprobante t
where c.estado != 'ANULADO' $P!{parametros}
and e.cod_cliente = c.cod_cliente
and t.cod_tipo = c.tipo
order by 2, c.fecha_nota_credito ]]></queryString>

