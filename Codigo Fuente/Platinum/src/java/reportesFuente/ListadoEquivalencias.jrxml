	<queryString><![CDATA[select  
        pFin.Cod_Producto as CodProductoFinal,
        pFin.Descripcion as DesProductoFinal, 
        m.nombre as Marca, 
        uFin.Descripcion as UnidadMedida, 
        pFin.Contenido as Contenido,
        e.relacion as Relacion,
        pGen.Cod_Producto as CodProductoGenerico,
        pGen.Descripcion as DesProductoGenerico
    
from equivalencia e
join producto pGen on e.cod_producto_gen = pGen.Cod_Producto
join producto pFin on e.cod_producto_fin = pFin.Cod_Producto
join unidad_medida uFin on pFin.Cod_Unidad_Medida = uFin.Cod_Unidad_Medida
join marca m on pFin.Cod_Marca = m.cod_marca
join presentacion pre on pFin.Cod_Presentacion = pre.cod_presentacion
where 1=1 $P!{parametros}

order by pFin.Descripcion, pGen.Cod_Producto, pFin.Cod_Producto]]></queryString>

