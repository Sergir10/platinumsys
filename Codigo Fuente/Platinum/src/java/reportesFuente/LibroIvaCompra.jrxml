	<queryString><![CDATA[select *
from 
(
select        
       cab.fecha          as fecha_comprobante, 
       p.ruc_proveedor    as ruc,
       t.desc_tipo        as desc_comprobante,
       cab.nro_factura    as nro_comprobante,
       cab.total          as total_comprobante,
       (case det.porc_iva
           when 10 then (det.precio_uni * det.cantidad)
           else 0
        end) as importe_sin_iva_10,
       (case det.porc_iva
           when 10 then det.monto_iva
           else 0
        end) as importe_iva_10,
       (case det.porc_iva
           when 5 then (det.precio_uni * det.cantidad)
           else 0
        end) as importe_sin_iva_5,
       (case det.porc_iva
           when 5 then det.monto_iva
           else 0
        end) as importe_iva_5,
       (case det.porc_iva
           when 0 then det.total
           else 0
        end) as excenta
        
           
  from factura_compra_cab cab
         inner join factura_compra_det det 
           on det.cod_fac_com_cab = cab.cod_fac_com_cab
         inner join tipo_comprobante   t 
           on t.cod_tipo = cab.tipo
         inner join proveedor          p 
           on p.cod_proveedor = cab.cod_proveedor
 where cab.estado != 'A'
 
 union all 
 select        
       nc.fecha             as fecha_comprobante, 
       p.ruc_proveedor      as ruc,
       t.desc_tipo          as desc_comprobante,
       nc.nro_nota_credito  as nro_comprobante,
       nc.total             as total_comprobante,
       (case nd.porcentaje_iva
           when 10 then (nd.precio_unitario * nd.cantidad)
           else 0
        end) as importe_sin_iva_10,
       (case nd.porcentaje_iva
           when 10 then nd.total_iva
           else 0
        end) as importe_iva_10,
        (case nd.porcentaje_iva
           when 5 then (nd.precio_unitario * nd.cantidad)
           else 0
        end) as importe_sin_iva_5,
       (case nd.porcentaje_iva
           when 5 then nd.total_iva
           else 0
        end) as importe_iva_5,
       (case nd.porcentaje_iva
           when 0 then nd.monto
           else 0
        end) as excenta
  from nota_credito_prov_cab nc
         inner join nota_credito_cli_detalle nd 
           on nd.cod_nota_credito_cliente = nc.cod_not_cre_cab
         inner join tipo_comprobante   t 
           on t.cod_tipo = nc.tipo
         inner join proveedor          p 
           on p.cod_proveedor = nc.cod_proveedor
 where nc.estado != 'ANULADO'
 ) aux
 order by aux.fecha_comprobante, 
          aux.desc_comprobante,
          aux.nro_comprobante]]></queryString>

