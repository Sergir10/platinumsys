	<queryString><![CDATA[select * from (
select cab.fecha_alta as Fecha, 
  dep.nombre as Deposito, 
    case when det.tipo_entrada_salida = 'E' then 'Entrada' else 'Salida' end as TipoMovimiento, 
      det.cod_producto as CodProducto, 
        p.descripcion as Descripcion, 
          m.nombre as Marca, 
            det.cantidad_ent_sal as Cantidad,
             case when det.tipo_entrada_salida = 'E' then det.cantidad_ent_sal else det.cantidad_ent_sal*-1 end as Valor

from Entrada_Salida_Detalle det
     join producto p on det.cod_producto = p.cod_producto 
     join marca m on p.cod_marca = m.cod_marca
     join entrada_salida_cabecera cab on det.cod_entrada_salida = cab.cod_entrada_salida
     join deposito dep on cab.cod_deposito = dep.cod_deposito
where det.cod_producto = $P!{codProd}
      and dep.cod_deposito = $P!{codDep}
      and cab.fecha_alta >= to_date($P!{fechaIni},'dd/mm/yyyy')
      and cab.fecha_alta <= to_date($P!{fechaFin},'dd/mm/yyyy')     
union 

select f.fecha_alta as Fecha,
   dep.nombre as Deposito,
    'Salida' as TipoMovimiento,
     f.cod_producto as CodProducto,
        p1.descripcion as Descripcion,
          m1.nombre as Marca,
            f.cantidad as Cantidad,
              f.cantidad * -1 as Valor
            
from factura_detalle f      
     join producto p1 on f.cod_producto = p1.cod_producto 
     join factura_cabecera fc on f.cod_factura = fc.cod_factura 
     join deposito dep on fc.cod_deposito = dep.cod_deposito
     join marca m1 on p1.cod_marca = m1.cod_marca
     where f.cod_producto = $P!{codProd}
	and fc.cod_deposito = $P!{codDep}      
	and fc.fecha_alta >= to_date($P!{fechaIni},'dd/mm/yyyy')
     	and fc.fecha_alta < to_date($P!{fechaFin},'dd/mm/yyyy')
)
where 1=1
order by 2,1]]></queryString>

