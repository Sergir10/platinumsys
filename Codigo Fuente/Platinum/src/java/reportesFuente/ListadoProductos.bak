	<queryString><![CDATA[select p.cod_producto as Cod, 
p.descripcion as Descrip, 
t.descripcion as Tipo, 
m.nombre as Marca, 
pr.descripcion as Presentacion, 
u.descripcion as Unidad,
p.contenido as Contenido, 
p.precio_actual as Precio, 
p.costo_actual as costo

from producto p
  join tipo_producto t on p.cod_tipo_producto = t.cod_tipo_producto
  join marca m on p.cod_marca = m.cod_marca
  join presentacion pr on p.cod_presentacion = pr.cod_presentacion
  join unidad_medida u on p.cod_unidad_medida = u.cod_unidad_medida


  where p.cod_tipo_producto != 6 and p.cod_tipo_producto != 1 $P!{parametros}
  order by t.descripcion, p.cod_producto
]]></queryString>

