	<queryString><![CDATA[select rownum as pos, x.cod_cliente, x.cliente, x.total_compra
  from
      ( select c.cod_cliente as cod_cliente, c.apellido_cliente||', '||c.nombre_cliente as cliente, sum(d.cantidad * d.precio_unitario) total_compra
          from factura_cabecera cab, factura_detalle d, cliente c
         where cab.estado_factura != 'ANULADO'
           and d.cod_factura = cab.cod_factura
           and c.cod_cliente = cab.cod_cliente
